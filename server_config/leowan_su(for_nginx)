
server {
    
    listen 80;
    listen [::]:80;

    server_name leowan.su www.leowan.su;

    include /etc/nginx/favicon_params;

    root /var/www/html;
    index sub.leowan.su.html;

}


server {

    listen 80;
    listen [::]:80;

    server_name megano.leowan.su www.megano.leowan.su;

    include /etc/nginx/favicon_params;

    location / {
        include /etc/nginx/proxy_params;
        proxy_pass http://unix:/run/gunicorn_megano.sock;
    }
    location /static/ {
        root /home/leowan/PyProjects/sb_megano/;
    }
    location /media/ {
        root /home/leowan/PyProjects/sb_megano/;
    }
}


#  sudo ln -s /etc/nginx/sites-available/leowan_su /etc/nginx/sites-enabled/
